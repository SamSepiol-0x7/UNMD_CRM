<UserControl
	x:Class="ASC.View.ACP.VoIPView" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:asc="clr-namespace:ASC" xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" xmlns:sys="clr-namespace:System;assembly=mscorlib" xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" xmlns:collections="clr-namespace:System.Collections;assembly=mscorlib" xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm" xmlns:dxdiag="http://schemas.devexpress.com/winfx/2008/xaml/diagram" xmlns:extensions="clr-namespace:ASC.Extensions" xmlns:conv="clr-namespace:ASC.Converters" xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui" xmlns:acp="clr-namespace:ASC.ViewModels.ACP" xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol" xmlns:acp1="clr-namespace:ASC.View.ACP">
	<FrameworkElement.DataContext>
		<acp:VoIPViewModel />
	</FrameworkElement.DataContext>
	<FrameworkElement.Resources>
		<ResourceDictionary>
			<asc:BindingProxy
				Data="{Binding}"
				x:Key="DataContextProxy" />
			<conv:BoolInverterConverter
				x:Key="BoolInverterConverter" />
		</ResourceDictionary>
	</FrameworkElement.Resources>
	<dxmvvm:Interaction.Behaviors>
		<dxwui:WinUIMessageBoxService
			WinUIMessageBoxService.Name="MessageService"
			x:Name="MessageService" />
	</dxmvvm:Interaction.Behaviors>
	<Grid>
		<dx:DXTabControl
			HorizontalAlignment="Stretch"
			VerticalAlignment="Stretch"
			Style="{StaticResource deDefaultTabControl}"
			p20:SelectorBase_x0060_2.SelectedIndex="{Binding VoipTabIndex, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" xmlns:p20="http://schemas.devexpress.com/winfx/2008/xaml/core/internal">
			<dx:DXTabControl.View>
				<dx:TabControlScrollView
					HeaderLocation="Left"
					HeaderOrientation="Vertical"
					AllowAnimation="True"
					AllowScrollOnMouseWheel="True"
					HideButtonShowMode="NoWhere"
					ShowHeaderMenu="False" />
			</dx:DXTabControl.View>
			<dx:DXTabItem
				HeaderedSelectorItemBase_x0060_2.Header="{DynamicResource Dialplan}">
				<Grid>
					<dxlc:LayoutControl
						Orientation="Vertical">
						<dxlc:LayoutGroup
							Orientation="Horizontal"
							VerticalAlignment="Top">
							<dxlc:LayoutGroup
								VerticalAlignment="Stretch"
								HorizontalAlignment="Left"
								Orientation="Vertical"
								View="GroupBox"
								Header="{DynamicResource IntPhoneN}">
								<dxlc:LayoutItem
									MaxHeight="250">
									<dxe:ListBoxEdit
										MinHeight="100"
										MinWidth="100"
										DisplayMember="id"
										SelectedItem="{Binding SelectedEndpoint, Mode=TwoWay}"
										ItemsSource="{Binding Endpoints}">
										<dxmvvm:Interaction.Behaviors>
											<dxmvvm:EventToCommand
												EventTriggerBase_x0060_1.EventName="MouseDoubleClick"
												Command="{Binding ShowEndpointEditCommand}" />
										</dxmvvm:Interaction.Behaviors>
									</dxe:ListBoxEdit>
								</dxlc:LayoutItem>
								<dxlc:LayoutItem>
									<TextBlock
										Foreground="#FF0000FF"
										Text="{DynamicResource EditHelp}"
										TextWrapping="Wrap" />
								</dxlc:LayoutItem>
								<dxlc:LayoutItem
									HorizontalContentAlignment="Right">
									<StackPanel
										Orientation="Horizontal">
										<dx:SimpleButton
											Content="{DynamicResource Delete}"
											Command="{Binding DeleteEndpointCommand}"
											Glyph="{DXImageGrayscale Image=Delete_16x16.png}" />
										<dx:SimpleButton
											Content="{DynamicResource Create}"
											Margin="5,0,0,0"
											Command="{Binding ShowCreateEndpointCommand}"
											Glyph="{DXImageGrayscale Image=Add_16x16.png}" />
									</StackPanel>
								</dxlc:LayoutItem>
							</dxlc:LayoutGroup>
							<dxlc:LayoutGroup
								Orientation="Vertical"
								VerticalAlignment="Stretch"
								Header="{DynamicResource CallSources}"
								View="GroupBox"
								HorizontalAlignment="Left">
								<dxlc:LayoutItem>
									<ListBox
										Width="150"
										MinHeight="100"
										ItemsSource="{Binding Contextsts}"
										SelectedItem="{Binding SelectedContext, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
								</dxlc:LayoutItem>
							</dxlc:LayoutGroup>
							<dxlc:LayoutGroup
								Orientation="Vertical"
								View="GroupBox"
								Header="{DynamicResource Dialplan}"
								IsEnabled="{Binding DialplanSelected}">
								<dxlc:LayoutItem
									MaxHeight="300">
									<dxg:GridControl
										MinHeight="100"
										IsFilterEnabled="False"
										Margin="0,0,0,33"
										ItemsSource="{Binding ContextDialplan}">
										<dxg:GridControl.View>
											<dxg:TableView
												ShowGroupPanel="False"
												AllowGrouping="False"
												AllowColumnMoving="False"
												AllowSorting="False"
												AllowColumnFiltering="False"
												AllowFilterEditor="False"
												ShowSearchPanelMode="Never" />
										</dxg:GridControl.View>
										<dxg:GridColumn
											GroupIndex="0"
											FieldName="Расширение"
											HorizontalHeaderContentAlignment="Center"
											Binding="{Binding exten}" />
										<dxg:GridColumn
											FieldName="Очерёдность"
											HorizontalHeaderContentAlignment="Center"
											Binding="{Binding priority}" />
										<dxg:GridColumn
											FieldName="App"
											Binding="{Binding app}" />
										<dxg:GridColumn
											FieldName="App data"
											Width="250*"
											Binding="{Binding appdata}" />
									</dxg:GridControl>
								</dxlc:LayoutItem>
								<dxlc:LayoutItem>
									<StackPanel
										Orientation="Horizontal"
										VerticalAlignment="Bottom"
										HorizontalAlignment="Right">
										<Button
											Margin="5,0,0,0"
											HorizontalAlignment="Right"
											VerticalAlignment="Bottom"
											Command="{Binding ClearDialPlanCommand}">
											<StackPanel
												Orientation="Horizontal">
												<Image
													Source="../../Resources/Remove.png"
													Style="{StaticResource 16X16Image}" />
												<Label
													Content="{DynamicResource Clear}"
													Margin="5,0,0,0" />
											</StackPanel>
										</Button>
										<dx:SimpleButton
											Style="{StaticResource SaveButton}"
											Margin="10,0,0,0"
											Command="{Binding SaveDialplanCommand}" />
									</StackPanel>
								</dxlc:LayoutItem>
							</dxlc:LayoutGroup>
						</dxlc:LayoutGroup>
						<dxlc:LayoutGroup
							View="GroupBox"
							Header="{DynamicResource DialplanEditor}"
							Orientation="Vertical"
							VerticalAlignment="Stretch">
							<dxlc:LayoutItem
								VerticalAlignment="Top">
								<StackPanel
									Orientation="Horizontal"
									VerticalAlignment="Top"
									HorizontalAlignment="Left">
									<Label
										VerticalContentAlignment="Center"
										Content="{DynamicResource ToExten}"
										ContentStringFormat="{}{0}:"
										Margin="5,0,0,0" />
									<dxe:ComboBoxEdit
										Width="130"
										Margin="5,0,0,0"
										ItemsSource="{Binding DialplanExtens}"
										EditValue="{Binding SelectedDialplanExten, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
										<dxmvvm:Interaction.Behaviors>
											<dxmvvm:EventToCommand
												EventTriggerBase_x0060_1.EventName="PreviewMouseLeftButtonDown"
												PassEventArgsToCommand="True"
												Command="{Binding ComboBoxClickCommand}" />
										</dxmvvm:Interaction.Behaviors>
									</dxe:ComboBoxEdit>
									<StackPanel
										Orientation="Horizontal"
										Visibility="{Binding AfterExtenVisibility}">
										<Label
											VerticalContentAlignment="Center"
											Content="{DynamicResource After}"
											ContentStringFormat="{}{0}:"
											Margin="5,0,0,0" />
										<dxe:ComboBoxEdit
											DisplayMember="Actions.Name"
											Width="130"
											Margin="5,0,0,0"
											ItemsSource="{Binding ExistDialplanActions}"
											EditValue="{Binding SelectedExistDialplanActions, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
											<dxmvvm:Interaction.Behaviors>
												<dxmvvm:EventToCommand
													EventTriggerBase_x0060_1.EventName="PreviewMouseLeftButtonDown"
													PassEventArgsToCommand="True"
													Command="{Binding ComboBoxClickCommand}" />
											</dxmvvm:Interaction.Behaviors>
										</dxe:ComboBoxEdit>
									</StackPanel>
									<Label
										VerticalContentAlignment="Center"
										Content="{DynamicResource Add}"
										ContentStringFormat="{}{0}:"
										Margin="5,0,0,0" />
									<dxe:ComboBoxEdit
										Width="130"
										Margin="5,0,0,0"
										DisplayMember="Name"
										ItemsSource="{Binding AllDialplanActions}"
										EditValue="{Binding NewDialplanAction, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
										<dxmvvm:Interaction.Behaviors>
											<dxmvvm:EventToCommand
												EventTriggerBase_x0060_1.EventName="PreviewMouseLeftButtonDown"
												PassEventArgsToCommand="True"
												Command="{Binding ComboBoxClickCommand}" />
										</dxmvvm:Interaction.Behaviors>
									</dxe:ComboBoxEdit>
									<StackPanel
										Orientation="Horizontal"
										Margin="5,0,0,0"
										HorizontalAlignment="Center"
										Visibility="{Binding ActionToTrunkVis}">
										<Label
											Content="{DynamicResource Trunks}"
											VerticalAlignment="Center"
											ContentStringFormat="{}{0}:" />
										<dxe:ComboBoxEdit
											Margin="5,0,0,0"
											DisplayMember="id"
											Width="130"
											ItemsSource="{Binding ZadarmaEndpointses}"
											EditValue="{Binding SelectedTrunk, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
											<dxmvvm:Interaction.Behaviors>
												<dxmvvm:EventToCommand
													EventTriggerBase_x0060_1.EventName="PreviewMouseLeftButtonDown"
													PassEventArgsToCommand="True"
													Command="{Binding ComboBoxClickCommand}" />
											</dxmvvm:Interaction.Behaviors>
										</dxe:ComboBoxEdit>
									</StackPanel>
									<StackPanel
										Orientation="Horizontal"
										Margin="5,0,0,0"
										HorizontalAlignment="Center"
										Visibility="{Binding ActionToDongleVis}">
										<Label
											Content="{DynamicResource UsbModems}"
											VerticalAlignment="Center"
											ContentStringFormat="{}{0}:" />
										<ComboBox
											Margin="5,0,0,0"
											DisplayMemberPath="Name"
											SelectedValuePath="Name"
											Width="130"
											ItemsSource="{Binding DongleDevicesConfig}"
											SelectedValue="{Binding SelectedDialDongle, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
									</StackPanel>
									<StackPanel
										Orientation="Horizontal"
										Margin="5,0,0,0"
										HorizontalAlignment="Center"
										Visibility="{Binding ActionToQueueVis}">
										<Label
											Content="{DynamicResource Queues}"
											VerticalAlignment="Center"
											ContentStringFormat="{}{0}:" />
										<ComboBox
											DisplayMemberPath="name"
											Width="100"
											Margin="5,0,0,0"
											ItemsSource="{Binding Queues}"
											SelectedItem="{Binding SelectedDialplanQueue, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
									</StackPanel>
									<StackPanel
										Orientation="Horizontal"
										Margin="5,0,0,0"
										HorizontalAlignment="Center"
										Visibility="{Binding ActionPlayFileVis}">
										<Label
											Content="{DynamicResource Path}"
											VerticalAlignment="Center"
											ContentStringFormat="{}{0}:" />
										<dxe:TextEdit
											IsEnabled="False"
											Width="130"
											Margin="5,0,0,0"
											EditValue="{Binding NewDialplanActionFilePath, Mode=TwoWay}" />
										<Button
											Margin="5,0,0,0"
											Content="{DynamicResource SelectFile}"
											Command="{Binding SelectAudioFileCommand}" />
									</StackPanel>
									<StackPanel
										Orientation="Horizontal"
										Margin="5,0,0,0"
										HorizontalAlignment="Center"
										Visibility="{Binding ActionSaveSmsVis}">
										<Label
											Content="{DynamicResource AsteriskDbPassword}"
											VerticalAlignment="Center"
											ContentStringFormat="{}{0}:" />
										<dxe:TextEdit
											Width="130"
											Margin="5,0,0,0"
											EditValue="{Binding NewDialplanActionAsteriskDbPassword, Mode=TwoWay}" />
									</StackPanel>
									<StackPanel
										Orientation="Horizontal"
										Margin="5,0,0,0"
										HorizontalAlignment="Center"
										Visibility="{Binding ActionToIntNumberVis}">
										<Label
											Content="{DynamicResource IntPhoneN}"
											VerticalAlignment="Center"
											ContentStringFormat="{}{0}:" />
										<ComboBox
											Margin="5,0,0,0"
											Width="150"
											DisplayMemberPath="IntPhoneAndFio"
											SelectedValue="id"
											IsEnabled="{Binding NewDialplanActionSipUserCalled, Converter={StaticResource BoolInverterConverter}}"
											SelectedItem="{Binding NewDialplanActionSipUser, Mode=TwoWay}"
											ItemsSource="{Binding DialplanSipUsers}" />
										<CheckBox
											VerticalAlignment="Center"
											Margin="5,0,0,0"
											Content="{DynamicResource CalledNumber}"
											IsChecked="{Binding NewDialplanActionSipUserCalled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
									</StackPanel>
									<dx:SimpleButton
										Style="{StaticResource SaveButton}"
										Margin="5,0,0,0"
										Command="{Binding AddDialplanActionCommand}" />
								</StackPanel>
							</dxlc:LayoutItem>
							<dxlc:LayoutItem
								VerticalAlignment="Stretch"
								MaxHeight="800">
								<dxdiag:DiagramControl
									MinHeight="200"
									TreeLayoutDirection="LeftToRight"
									ToolboxVisibility="Closed"
									SelectedStencils="BasicShapes, ArrowShapes"
									ShowGrid="False"
									ShowPageBreaks="False"
									ShowRulers="False"
									CanvasSizeMode="Fill">
									<dxmvvm:Interaction.Behaviors>
										<extensions:DiagramOrgChartBehaviorEx
											dxdiag:ChildrenPath="Children"
											dxdiag:ItemsSource="{Binding ExtenVisObjs}">
											<dxdiag:DiagramDataBindingBehaviorBase.TemplateDiagram>
												<dxdiag:DiagramControl
													CanvasSizeMode="Fill"
													SelectedStencils="TemplateDesigner, BasicShapes"
													ShowPageBreaks="False"
													Theme="Linear">
													<dxdiag:DiagramConnector
														CanDelete="False"
														CanEdit="False"
														CanCopyWithoutParent="False"
														CanMove="False"
														CanSelect="False"
														CanDeleteWithoutParent="False"
														CanCopy="False"
														Points="(Empty)"
														ThemeStyleId="ConnectorId.Subtle1"
														Type="Curved" />
													<dxdiag:DiagramContainer
														Anchors="Left, Top"
														CanDelete="False"
														CanCopyWithoutParent="True"
														ConnectionPoints="0.5,0 1,0.5 0.5,1 0,0.5"
														CanAddItems="False"
														CanCopy="False"
														DragMode="ByAnyPoint"
														Height="105"
														ItemsCanChangeParent="False"
														ItemsCanAttachConnectorEndPoint="False"
														ItemsCanAttachConnectorBeginPoint="False"
														ItemsCanSelect="False"
														ItemsCanEdit="False"
														ItemsCanCopyWithoutParent="False"
														ItemsCanMove="False"
														ItemsCanRotate="False"
														ItemsCanSnapToOtherItems="False"
														ItemsCanDeleteWithoutParent="False"
														ItemsCanSnapToThisItem="False"
														ItemsCanResize="False"
														Position="350,30"
														ThemeStyleId="Variant1"
														TemplateName="QueueTemplate"
														Width="150">
														<dxdiag:DiagramShape
															Anchors="Left, Right, Bottom"
															FontSize="14"
															Height="45"
															Position="10,20"
															StrokeThickness="0"
															VerticalContentAlignment="Top"
															Width="130">
															<dxdiag:DiagramItem.Bindings>
																<dxdiag:DiagramBinding
																	Expression="Name"
																	PropertyName="Content" />
															</dxdiag:DiagramItem.Bindings>
														</dxdiag:DiagramShape>
														<dxdiag:DiagramShape
															Anchors="Left, Top, Right"
															Background="#001BA1E2"
															FontSize="11"
															Height="43"
															Position="10,40"
															StrokeThickness="0"
															Width="130">
															<dxdiag:DiagramItem.Bindings>
																<dxdiag:DiagramBinding
																	Expression="AdditionalInfo"
																	PropertyName="Content" />
															</dxdiag:DiagramItem.Bindings>
														</dxdiag:DiagramShape>
														<dxdiag:DiagramShape
															ToolTip="{DynamicResource Priority}"
															Anchors="Left, Top, Right"
															Background="#001BA1E2"
															FontSize="11"
															Height="43"
															Position="10,60"
															StrokeThickness="0"
															Width="130">
															<dxdiag:DiagramItem.Bindings>
																<dxdiag:DiagramBinding
																	Expression="Priority"
																	PropertyName="Content" />
															</dxdiag:DiagramItem.Bindings>
														</dxdiag:DiagramShape>
													</dxdiag:DiagramContainer>
												</dxdiag:DiagramControl>
											</dxdiag:DiagramDataBindingBehaviorBase.TemplateDiagram>
										</extensions:DiagramOrgChartBehaviorEx>
									</dxmvvm:Interaction.Behaviors>
								</dxdiag:DiagramControl>
							</dxlc:LayoutItem>
							<dxlc:LayoutItem
								VerticalAlignment="Bottom">
								<StackPanel
									Orientation="Horizontal"
									VerticalAlignment="Bottom"
									HorizontalAlignment="Left">
									<Label
										VerticalContentAlignment="Center"
										Content="{DynamicResource SelectExten}"
										ContentStringFormat="{}{0}:" />
									<dxe:ComboBoxEdit
										Width="130"
										Margin="5,0,0,0"
										ItemsSource="{Binding DialplanExtens}"
										EditValue="{Binding SelectedDialplanExten, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
										<dxmvvm:Interaction.Behaviors>
											<dxmvvm:EventToCommand
												EventTriggerBase_x0060_1.EventName="PreviewMouseLeftButtonDown"
												PassEventArgsToCommand="True"
												Command="{Binding ComboBoxClickCommand}" />
										</dxmvvm:Interaction.Behaviors>
									</dxe:ComboBoxEdit>
									<Label
										VerticalContentAlignment="Center"
										Content="{DynamicResource ExistActions}"
										ContentStringFormat="{}{0}:"
										Margin="5,0,0,0" />
									<dxe:ComboBoxEdit
										DisplayMember="Actions.Name"
										Width="130"
										Margin="5,0,0,0"
										ItemsSource="{Binding ExistDialplanActions}"
										EditValue="{Binding SelectedExistDialplanActions, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
										<dxmvvm:Interaction.Behaviors>
											<dxmvvm:EventToCommand
												EventTriggerBase_x0060_1.EventName="PreviewMouseLeftButtonDown"
												PassEventArgsToCommand="True"
												Command="{Binding ComboBoxClickCommand}" />
										</dxmvvm:Interaction.Behaviors>
									</dxe:ComboBoxEdit>
									<Button
										Margin="5,0,0,0"
										Command="{Binding RemoveDialplanActionCommand}">
										<StackPanel
											Orientation="Horizontal">
											<Image
												Source="../../Resources/Remove.png"
												Style="{StaticResource 16X16Image}" />
											<Label
												Content="{DynamicResource Delete}"
												Margin="10,0,0,0" />
										</StackPanel>
									</Button>
								</StackPanel>
							</dxlc:LayoutItem>
						</dxlc:LayoutGroup>
					</dxlc:LayoutControl>
				</Grid>
			</dx:DXTabItem>
			<dx:DXTabItem
				HeaderedSelectorItemBase_x0060_2.Header="{DynamicResource Queues}">
				<dxlc:LayoutControl
					Orientation="Vertical">
					<dxlc:LayoutGroup
						Header="{DynamicResource Queues}"
						VerticalAlignment="Top"
						View="GroupBox"
						Orientation="Horizontal">
						<dxlc:LayoutGroup
							Orientation="Vertical"
							HorizontalAlignment="Left">
							<dxlc:LayoutItem>
								<StackPanel
									Orientation="Horizontal"
									HorizontalAlignment="Left"
									VerticalAlignment="Top">
									<Label
										Content="{DynamicResource Name}"
										ContentStringFormat="{}{0}:" />
									<dxe:TextEdit
										Mask="[a-zA-Z0-9\/\_\-]+"
										MaskType="RegEx"
										Width="90"
										Margin="5,0,0,0"
										EditValue="{Binding NewQueueName, Mode=TwoWay}" />
									<Button
										Content="{DynamicResource Add}"
										Margin="5,0,0,0"
										Command="{Binding AddQueueCommand}" />
								</StackPanel>
							</dxlc:LayoutItem>
							<dxlc:LayoutGroup
								View="GroupBox"
								Header="{DynamicResource QueuesList}">
								<dxlc:LayoutItem>
									<ListBox
										DisplayMemberPath="name"
										HorizontalAlignment="Stretch"
										VerticalAlignment="Top"
										Height="200"
										ItemsSource="{Binding Queues}"
										SelectedItem="{Binding SelectedQueue}" />
								</dxlc:LayoutItem>
							</dxlc:LayoutGroup>
						</dxlc:LayoutGroup>
						<dxlc:LayoutGroup
							Orientation="Vertical"
							View="GroupBox"
							HorizontalAlignment="Left"
							Header="{DynamicResource QueueSettings}"
							IsEnabled="{Binding CanEditQueue, Mode=OneWay}">
							<dxlc:LayoutItem>
								<StackPanel
									Orientation="Vertical"
									HorizontalAlignment="Left"
									VerticalAlignment="Top">
									<StackPanel
										Orientation="Horizontal"
										HorizontalAlignment="Right">
										<Label
											Content="{DynamicResource QueueTimeout}"
											ContentStringFormat="{}{0}:" />
										<dxe:SpinEdit
											HorizontalContentAlignment="Center"
											Width="100"
											MinValue="5"
											IsFloatValue="False"
											ShowError="False"
											Margin="5,0,0,0"
											EditValue="{Binding SelectedQueue.timeout, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
									</StackPanel>
									<StackPanel
										Orientation="Horizontal"
										Margin="0,5,0,0"
										HorizontalAlignment="Right">
										<Label
											Content="{DynamicResource Ringinuse}"
											ContentStringFormat="{}{0}:" />
										<dxe:ComboBoxEdit
											HorizontalContentAlignment="Center"
											Width="100"
											ShowError="False"
											Margin="5,0,0,0"
											EditValue="{Binding SelectedQueue.ringinuse, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
											<dxe:LookUpEditBase.ItemsSource>
												<collections:ArrayList>
													<sys:String>yes</sys:String>
													<sys:String>no</sys:String>
												</collections:ArrayList>
											</dxe:LookUpEditBase.ItemsSource>
										</dxe:ComboBoxEdit>
									</StackPanel>
									<StackPanel
										Orientation="Horizontal"
										Margin="0,5,0,0"
										HorizontalAlignment="Right">
										<Label
											Content="{DynamicResource QueueAutofill}"
											ContentStringFormat="{}{0}:" />
										<dxe:ComboBoxEdit
											HorizontalContentAlignment="Center"
											Width="100"
											ShowError="False"
											Margin="5,0,0,0"
											EditValue="{Binding SelectedQueue.autofill, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
											<dxe:LookUpEditBase.ItemsSource>
												<collections:ArrayList>
													<sys:String>yes</sys:String>
													<sys:String>no</sys:String>
												</collections:ArrayList>
											</dxe:LookUpEditBase.ItemsSource>
										</dxe:ComboBoxEdit>

									HorizontalAlignment="Right"
									Margin="0,5,0,0">
									<Label
										Content="{DynamicResource PhoneShort}"
										ContentStringFormat="{}{0}:" />
									<dxe:TextEdit
										MaskType="Numeric"
										Mask="d"
										Width="130"
										Margin="5,0,0,0"
										EditValue="{Binding NewZadarmaAorContact, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
								</StackPanel>
								<Button
									Content="{DynamicResource Add}"
									Margin="0,5,0,0"
									Command="{Binding AddZadarmaTrunkCommand}" />
								<ListBox
									Margin="0,5,0,0"
									HorizontalAlignment="Stretch"
									MinHeight="200"
									DisplayMemberPath="id"
									Height="168"
									ItemsSource="{Binding ZadarmaEndpointses}"
									SelectedItem="{Binding SelectedZadarmaEndpoint, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
							</StackPanel>
						</Grid>
					</GroupBox>
					<GroupBox
						Header="{DynamicResource Settings}"
						HorizontalAlignment="Left"
						VerticalAlignment="Top"
						Margin="228,10,0,0"
						IsEnabled="{Binding TrunkSelected, Mode=OneWay}">
						<Grid>
							<StackPanel
								Orientation="Vertical"
								HorizontalAlignment="Right"
								VerticalAlignment="Top">
								<StackPanel
									Orientation="Horizontal"
									HorizontalAlignment="Right">
									<Label
										Content="{DynamicResource SipTransport}"
										ContentStringFormat="{}{0}:" />
									<dxe:TextEdit
										Mask="[a-zA-Z0-9\/\-\_]+"
										MaskType="RegEx"
										IsEnabled="False"
										Width="150"
										Margin="5,0,0,0"
										EditValue="{Binding SelectedZadarmaEndpoint.transport}" />
								</StackPanel>
								<StackPanel
									Orientation="Horizontal"
									HorizontalAlignment="Right"
									Margin="0,5,0,0">
									<Label
										Content="{DynamicResource PhoneShort}"
										ContentStringFormat="{}{0}:" />
									<dxe:TextEdit
										MaskType="Numeric"
										Mask="d"
										Width="150"
										Margin="5,0,0,0"
										EditValue="{Binding SelectedZadarmaAor.ContactPhone, Mode=TwoWay}" />
								</StackPanel>
								<StackPanel
									Orientation="Horizontal"
									HorizontalAlignment="Right"
									Margin="0,5,0,0">
									<Label
										Content="{DynamicResource Context}"
										ContentStringFormat="{}{0}:" />
									<dxe:TextEdit
										Mask="[a-zA-Z0-9\/\-\_]+"
										MaskType="RegEx"
										Width="150"
										Margin="5,0,0,0"
										EditValue="{Binding SelectedZadarmaEndpoint.context, Mode=TwoWay}" />
								</StackPanel>
								<StackPanel
									Orientation="Horizontal"
									HorizontalAlignment="Right"
									Margin="0,5,0,0">
									<Label
										Content="{DynamicResource Codec}"
										ContentStringFormat="{}{0}:" />
									<dxe:ComboBoxEdit
										SeparatorString=","
										Width="150"
										Margin="5,0,0,0"
										EditValue="{Binding SelectedZadarmaEndpoint.AllowList, Mode=TwoWay}">
										<dxe:LookUpEditBase.ItemsSource>
											<collections:ArrayList>
												<sys:String>alaw</sys:String>
												<sys:String>ulaw</sys:String>
												<sys:String>g722</sys:String>
											</collections:ArrayList>
										</dxe:LookUpEditBase.ItemsSource>
										<dxe:BaseEdit.StyleSettings>
											<dxe:CheckedComboBoxStyleSettings />
										</dxe:BaseEdit.StyleSettings>
									</dxe:ComboBoxEdit>
								</StackPanel>
								<StackPanel
									Orientation="Horizontal"
									HorizontalAlignment="Right">
									<Button
										HorizontalAlignment="Right"
										Margin="0,5,0,0"
										Command="{Binding DeleteZadarmaTrunkCommand}">
										<StackPanel
											Orientation="Horizontal">
											<Image
												Source="../../Resources/Remove.png"
												Style="{StaticResource 16X16Image}" />
											<Label
												Content="{DynamicResource Delete}"
												Margin="10,0,0,0" />
										</StackPanel>
									</Button>
									<Button
										Margin="10,5,0,0"
										Style="{StaticResource ButtonSave}"
										Command="{Binding SaveZadarmaTrunkCommand}" />
								</StackPanel>
							</StackPanel>
						</Grid>
					</GroupBox>
				</Grid>
			</dx:DXTabItem>
		</dx:DXTabControl>
	</Grid>
</UserControl>
